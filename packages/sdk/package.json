{
   "name": "@malloy-publisher/sdk",
   "description": "Malloy Publisher SDK",
   "version": "0.0.138",
   "type": "module",
   "main": "dist/index.cjs.js",
   "module": "dist/index.es.js",
   "types": "dist/index.d.ts",
   "typesVersions": {
      "*": {
         "client": [
            "dist/client-entry.d.ts"
         ],
         "client/*": [
            "dist/client/*.d.ts"
         ]
      }
   },
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.es.js",
         "require": "./dist/index.cjs.js"
      },
      "./client": {
         "types": "./dist/client-entry.d.ts",
         "import": "./dist/client/index.es.js",
         "require": "./dist/client/index.cjs.js"
      },
      "./client/*": {
         "types": "./dist/client/*.d.ts",
         "import": "./dist/client/*.js"
      },
      "./malloy-explorer.css": {
         "default": "./dist/malloy-explorer.css"
      },
      "./markdown-editor.css": {
         "default": "./dist/markdown-editor.css"
      },
      "./styles.css": {
         "default": "./dist/styles.css"
      }
   },
   "style": "./dist/malloy-explorer.css",
   "sideEffects": [
      "**/*.css",
      "./src/index.ts",
      "./dist/index.*.js"
   ],
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "build": "bun generate-api-types && bunx vite build",
      "postbuild": "cp ../../node_modules/@malloydata/malloy-explorer/dist/malloy-explorer.css ./dist/malloy-explorer.css && cp ../../node_modules/@uiw/react-md-editor/markdown-editor.css ./dist/markdown-editor.css && echo '/* SDK Styles */\n@import \"./malloy-explorer.css\";\n@import \"./markdown-editor.css\";' > ./dist/styles.css",
      "test": "bun test",
      "lint": "bunx eslint ./src --fix",
      "format": "bunx prettier --write --parser typescript '**/*.{ts,tsx}'",
      "analyze": "bunx vite-bundle-visualizer",
      "generate-api-types": "bunx openapi-generator-cli generate -i ../../api-doc.yaml -g typescript-axios -o src/client/"
   },
   "peerDependencies": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0",
      "react-router-dom": "^7.6.2",
      "@tanstack/react-query": "^5.59.16"
   },
   "dependencies": {
      "@emotion/react": "^11.14.0",
      "@emotion/styled": "^11.14.0",
      "@malloydata/malloy-explorer": "^0.0.305-dev250819202105",
      "@malloydata/malloy-interfaces": "^0.0.307",
      "@malloydata/malloy-query-builder": "^0.0.307",
      "@malloydata/render": "^0.0.307",
      "@mui/icons-material": "^7.1.1",
      "@mui/material": "^7.1.1",
      "@mui/system": "^7.1.1",
      "@mui/x-tree-view": "^7.16.0",
      "@react-spring/web": "^10.0.1",
      "@uiw/react-md-editor": "^4.0.6",
      "axios": "^1.7.7",
      "markdown-to-jsx": "^7.7.6",
      "duckdb": "^1.3.1"
   },
   "devDependencies": {
      "@openapitools/openapi-generator-cli": "^2.20.2",
      "@types/bun": "^1.2.21",
      "@types/k6": "^1.0.2",
      "@types/react": "^19.1.7",
      "@types/react-dom": "^19.1.6",
      "@typescript-eslint/eslint-plugin": "8.16.0",
      "@typescript-eslint/parser": "8.16.0",
      "@vitejs/plugin-react": "^4.4.1",
      "ajv": "^8.12.0",
      "archiver": "^7.0.1",
      "eslint-config-prettier": "9.1.0",
      "eslint-plugin-prettier": "5.2.1",
      "eslint-plugin-react": "7.37.2",
      "eslint-plugin-react-hooks": "5.0.0",
      "eslint-plugin-storybook": "^0.11.4",
      "shiki": "^1.16.3",
      "vite": "^6.3.2",
      "vite-plugin-dts": "^4.5.3",
      "vite-plugin-svgr": "^4.3.0"
   }
}
