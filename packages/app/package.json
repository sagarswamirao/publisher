{
  "name": "@malloy-publisher/app",
  "description": "Malloy Publisher App",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build --mode ${NODE_ENV:=development}",
    "clean": "rm -rf ./dist && rm -rf ./node_modules && rm -f package-lock.json",
    "lint": "eslint ./src --ext .ts,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate-api-types": ""
  },
  "dependencies": {
    "@malloy-publisher/sdk": "^0.0.4",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.9",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  }
}
